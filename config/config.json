{
  "version": 1,
  "maintenance": {
    "workspace": "~/development",
    "idea-cache-dir": "~/.IntelliJIdea14/system"
  },
  "ms": {
    "workspace": "~/development/tradis"
  },
  "microservices": [
    {
      "name": "eureka",
      "slug": "infrastructure/eureka",
      "build": {
        "params": [
          "--skip-tests"
        ]
      },
      "run": {
        "javaopts": [
          "-Xmx64m",
          "-XX:MaxMetaspaceSize=100M"
        ],
        "params": {
          "spring.zipkin.enabled": false,
          "spring.profiles.active": "dev,local",
          "server.port": 8761
        }
      },
      "params": {
        "register-in-eureka": false
      }
    },
    {
      "name": "wallet",
      "slug": "business/wallet",
      "build": {
        "params": []
      },
      "run": {
        "javaopts": [
          "-Xmx200m",
          "-XX:MaxMetaspaceSize=200M"
        ],
        "params": {
          "spring.zipkin.enabled": false,
          "spring.cloud.config.uri": "http://localhost:8888/",
          "spring.cloud.config.label": "develop",
          "spring.profiles.active": "dev,local",
          "server.port": 8081
        }
      },
      "params": {
        "register-in-eureka": true
      }
    },
    {
      "name": "corporate-info"
    },
    {
      "name": "general-info"
    },
    {
      "name": "info-registro"
    },
    {
      "name": "pricing"
    },
    {
      "name": "publicidad"
    },
    {
      "name": "users"
    },
    {
      "name": "search-api"
    },
    {
      "name": "search-api"
    },
    {
      "name": "search-datastore"
    },
    {
      "name": "pdf-generator"
    },
    {
      "name": "template-store"
    },
    {
      "name": "zipkin-server"
    },
    {
      "name": "billing"
    },
    {
      "name": "publicidad-request"
    },
    {
      "name": "payment"
    },
    {
      "name": "publicidad-reply"
    },
    {
      "name": "file-manager"
    },
    {
      "name": "user-notificator"
    }
  ]
}